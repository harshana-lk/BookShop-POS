<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="90.0" prefWidth="120.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bookiebook.pos.controller.DropDownPanelController">
   <children>
      <VBox prefHeight="103.0" prefWidth="120.0" spacing="10.0">
         <children>
            <JFXButton fx:id="btnProfile" onAction="#profileOnAction" text="Profile" textFill="WHITE">
               <font>
                  <Font name="Consolas Bold" size="16.0" />
               </font>
               <graphic>
                  <FontAwesomeIconView fill="WHITE" glyphName="USER" size="18" />
               </graphic>
               <VBox.margin>
                  <Insets left="6.0" top="4.0" />
               </VBox.margin>
            </JFXButton>
            <Separator opacity="0.26" prefHeight="3.0" prefWidth="99.0" />
            <JFXButton fx:id="btnSettings" onAction="#settingsOnAction" text="Settings" textFill="WHITE">
               <font>
                  <Font name="Consolas Bold" size="16.0" />
               </font>
               <graphic>
                  <FontAwesomeIconView fill="WHITE" glyphName="GEAR" size="18" />
               </graphic>
               <VBox.margin>
                  <Insets left="6.0" />
               </VBox.margin>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
